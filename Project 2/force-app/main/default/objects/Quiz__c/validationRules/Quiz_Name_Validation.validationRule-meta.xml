<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quiz_Name_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(REGEX(Name, &quot;[A-Z]{1}[a-z]{4,50}&quot;))</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>The name of the quiz must start with a capital letter and also must have 5 to 50 characters.;</errorMessage>
</ValidationRule>
