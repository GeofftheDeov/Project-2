<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Question_Name_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(REGEX(Name, &quot;[A-Z]{1}[a-z]{4,200}&quot;))</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>The question must begin with a capital letter and also must be between 5 characters and 200 characters.</errorMessage>
</ValidationRule>
