    <label>Quiz</label>
    <nameField>
        <label>Quiz Name</label>
        <trackHistory>false</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Quizzes</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <visibility>Public</visibility>
    <fields>
        <fullName>Attempts_Made__c</fullName>
        <externalId>false</externalId>
        <label>Attempts Made</label>
        <summaryFilterItems>
            <field>AccountQuiz__c.Score__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </summaryFilterItems>
        <summaryForeignKey>AccountQuiz__c.Quiz__c</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Avg_Score__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Attempts_Made__c &gt;0, TotalScore__c / Attempts_Made__c, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Avg Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Questions__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Questions</label>
        <referenceTo>Questions__c</referenceTo>
        <relationshipLabel>Quizzes</relationshipLabel>
        <relationshipName>Quizzes</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Score__c</fullName>
        <externalId>false</externalId>
        <label>Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>TotalScore__c</fullName>
        <externalId>false</externalId>
        <label>Total Score</label>
        <summarizedField>AccountQuiz__c.Score__c</summarizedField>
        <summaryForeignKey>AccountQuiz__c.Quiz__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <validationRules>
        <fullName>First_Capital</fullName>
        <active>true</active>
        <errorConditionFormula>LEFT(UPPER( Name ),1) &lt;&gt; LEFT( Name ,1)</errorConditionFormula>
        <errorMessage>First letter not capitalized.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quiz_Name_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(REGEX(Name, &quot;[A-Z]{1}[a-z]{4,50}&quot;))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The name of the quiz must start with a capital letter and also must have 5 to 50 characters.;</errorMessage>
    </validationRules>
    <listViews>
