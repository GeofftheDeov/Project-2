<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>First_Capital</fullName>
    <active>true</active>
    <errorConditionFormula>LEFT(UPPER( Name ),1) &lt;&gt; LEFT( Name ,1)</errorConditionFormula>
    <errorMessage>First letter not capitalized.</errorMessage>
</ValidationRule>
